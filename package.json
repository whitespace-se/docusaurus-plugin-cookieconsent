{
  "name": "@whitespace-se/docusaurus-plugin-cookieconsent",
  "version": "1.0.0",
  "description": "Cookie consent plugin for Docusaurus with analytics integration support",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/",
    "src/"
  ],
  "keywords": [
    "docusaurus",
    "plugin",
    "cookie-consent",
    "gdpr",
    "privacy",
    "analytics"
  ],
  "author": "Whitespace AB <code@whitespace.se>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/whitespace-se/docusaurus-plugin-cookieconsent.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "@docusaurus/core": "^3.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@docusaurus/core": "^3.5.2",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "jsdom": "^26.1.0",
    "tdd-guard-vitest": "^0.1.3",
    "typescript": "^5.0.0",
    "vite": "^6.3.5"
  },
  "scripts": {
    "build": "tsc && find src -name '*.css' -type f -exec sh -c 'mkdir -p lib/$(dirname ${1#src/}) && cp $1 lib/${1#src/}' _ {} \\;",
    "build:watch": "tsc --watch",
    "clean": "rm -rf lib",
    "test": "VITE_CJS_IGNORE_WARNING=true vitest run",
    "test:watch": "VITE_CJS_IGNORE_WARNING=true vitest",
    "prepack": "npm run clean && npm run build"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
